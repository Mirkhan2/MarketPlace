@if (
	TempData["SuccessMessage"] != null ||
	TempData["ErrorMessage"] != null ||
	TempData["InfoMessage"] != null ||
	TempData["WarningMessage"] != null 
		)
{
	if(TempData["SuccessMessage"] != null)
	{
		<script>
			$(document).ready(function () {
				ShowMessage('peygham mowafaqiat ','@TempData["SuccessMessage"]', 'sucess');
			});
		</script>
		
	}
	if (TempData["ErrorMessage"] != null)
	{
		<script>
			$(document).ready(function () {
				ShowMessage('peygham khata ', '@TempData["ErrorMessage"]', 'error');
			});
		</script>

	}
	if (TempData["WarningMessage"] != null)
	{
		<script>
			$(document).ready(function () {
				ShowMessage('peygham Ekhtar', '@TempData["SuccessMessage"]', 'warning');
			});
		</script>

	}
	if (TempData["InfoMessage"] != null)
	{
		<script>
			$(document).ready(function () {
				ShowMessage('peygham Etelaei', '@TempData["SuccessMessage"]', 'info');
			});
		</script>

	}

}

